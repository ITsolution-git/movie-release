{
    "index": "/index.html",
    "assetGroups": [{
            "name": "app",
            "installMode": "prefetch",
            "resources": {
                "files": [
                    "/favicon.ico",
                    "/index.html"
                ],
                "versionedFiles": [
                    "/*.bundle.css",
                    "/*.bundle.js",
                    "/*.chunk.js"
                ]
            }
        },
        {
            "name": "assets",
            "installMode": "lazy",
            "updateMode": "prefetch",
            "resources": {
                "files": [
                    "/assets/**"
                ],
                "urls": [
                    "https://cdn.ckeditor.com/4.6.1/full-all/ckeditor.js",
                    "https://fonts.googleapis.com/css?family=Roboto",
                    "https://fonts.googleapis.com/icon?family=Material+Icons"
                ]
            }
        }
    ],
    "dataGroups": [{
            "name": "tmdb-api-freshness",
            "urls": [
                "/",
                "/movies",
                "/celebrities",
                "/search"
            ],
            "cacheConfig": {
                "maxSize": 100,
                "maxAge": "1h",
                "timeout": "5s",
                "strategy": "freshness"
            }
        },
        {
            "name": "tmdb-api-performance",
            "urls": [
                "/movies/*",
                "/celebrity"
            ],
            "cacheConfig": {
                "maxSize": 100,
                "maxAge": "4h",
                "strategy": "performance"
            }
        }
    ]
}